{
  "quoteProps": "as-needed",
  "singleQuote": true,
  "arrowParens": "always",
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 100,
  "proseWrap": "always",
  "bracketSpacing": false,
  "trailingComma": "es5",
  "semi": true,
  "jsxSingleQuote": false,
  "jsxBracketSameLine": false,
  "endOfLine": "auto",
  "plugins": [
    "prettier-plugin-tailwindcss"
  ]
}
